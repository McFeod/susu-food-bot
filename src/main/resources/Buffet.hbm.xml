<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "src/main/resources/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="logic">
    <class name="Buffet" table="buffets">
        <id column="buffet_id" name="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property column="name" name="name" type="java.lang.String" unique="true" not-null="true"/>
        <property column="admin" name="isAdmin" type="java.lang.Boolean" not-null="true"/>
        <property column="complained" name="isComplained" type="java.lang.Boolean" not-null="true"/>
    </class>
</hibernate-mapping>