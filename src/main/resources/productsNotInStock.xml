<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "src/main/resources/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="logic.ProductsNotInStock" table="products_not_in_stock">
        <id column="id" name="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <many-to-one name="buffet" class="logic.Buffet"
                     column="buffet_id" not-null="true"
                     cascade="all" lazy="false" fetch="select"/>
        <many-to-one name="product" class="logic.Product"
                     column="product_id" not-null="true"
                     cascade="all" lazy="false" fetch="select"/>

    </class>
</hibernate-mapping>